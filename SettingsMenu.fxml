      </ImageView>
      <ImageView fx:id="playBT" fitHeight="83.0" fitWidth="262.0" layoutX="81.0" layoutY="387.0" onMouseClicked="#setOnPlayClicked" onMouseEntered="#setOnHoverPlayBT" pickOnBounds="true" preserveRatio="true">
